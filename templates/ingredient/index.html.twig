{% extends "base.html.twig" %}

{% block body %}
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Name</th>
        </tr>
        </thead>
        <tbody>
        {% for ingredient in ingredients %}
            <tr>
                <td>{{ ingredient.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
